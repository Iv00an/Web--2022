# Внутри класса после метода будем писать строку с наименованием метода, который был вызван
class A:
    def __init__(self,value):
        self.value = value

    def __str__(self):
        return f'A({self.value}) object str method'
    
    def __repr__(self):
        return f'A({self.value}) object repr method'
    
    def __format__(self, format_spec):
        return f'A({self.value}) object format method'
    
    
a= A(365) # создаем объект класса
print(a) # вызываем над ним принт
print("some text wiht %s" % a) # вызываем  принт какой то строки, внутрь которой будет добавлен объект а, вызывается метод str
print(f'some text with {a}') # если форматируем строку, то вызывается метод формат
print("some text with {a}".format(a = a)) 
a # просто смотрим, вызывется метод repr
